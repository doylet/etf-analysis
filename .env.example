# Local Development Environment Variables
# Copy this file to .env and fill in your values

# Database
DATABASE_URL=sqlite:///./data/etf_analysis.db

# Use BigQuery for production (set to 'true' when deployed)
USE_BIGQUERY=false

# Alpha Vantage API (for symbol search)
ALPHAVANTAGE_API_KEY=your-api-key-here

# Google Cloud Project (for production)
GCP_PROJECT_ID=your-project-id
GCP_BUCKET_NAME=your-bucket-name

# Optional: Cloud SQL (for production)
# DATABASE_URL=postgresql://user:password@/dbname?host=/cloudsql/project:region:instance
